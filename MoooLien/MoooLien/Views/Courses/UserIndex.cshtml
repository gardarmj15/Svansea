@model MoooLien.Models.ViewModel.UsersCoursesViewModel

@{
    ViewBag.Title = "UsersInCourse";
    Layout = "~/Views/Shared/_LayoutUsers.cshtml";
}

<h2>My Courses</h2>

<div>
    <table class="table table-hover">
        <tbody>
            @foreach (var x in Model.courses)
            {
                <tr>
                    <td>
                        <a href="@Url.Action("Index", "Assignments", new { id = x.ID})">
                            @x.name
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


