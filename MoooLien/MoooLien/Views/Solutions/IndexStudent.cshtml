@model IEnumerable<MoooLien.Models.Entities.Assignment>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutUsers.cshtml";
}

<h2>My Assignments</h2>

<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="@Url.Action("Details", "Solutions", new { id = item.ID})"/> @Html.DisplayFor(modelItem => item.name)
            </td>
        </tr>
    }

</table>
