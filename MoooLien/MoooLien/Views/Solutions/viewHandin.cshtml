@model MoooLien.Models.ViewModel.SolutionViewModel

@{
    ViewBag.Title = "viewHandin";
    Layout = "~/Views/Shared/_LayoutUsers.cshtml";
}

<h2>Students Handins</h2>

@Html.AntiForgeryToken()
<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Status</td>
                <td>Date and time</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model.solutions)
            {
                <tr>
                    <td>@user.accepted</td>
                    <td>@user.handinDate</td>
                </tr>
            }
        </tbody>
    </table>
</div>

<div>
    @Html.ActionLink("Back to My Courses", "UserIndex", "Courses")
</div>